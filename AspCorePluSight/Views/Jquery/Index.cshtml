
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@section Header{

    <style>
        #draggable {
            width: 150px;
            height: 150px;
            padding: 0.5em;
            border-style:solid;
            border-color:cadetblue;
        }
    </style>
    <script>
        $(function () {
            $("#draggable").draggable();
        });
    </script>

}
<h2>Index</h2>

<div id="draggable" class="ui-widget-content">
    <p>Drag me around</p>
</div>

